<%@page import="com.jda.model.UserModel"%>
<html>
<head>
</head>
<body>
	<%
		String username = "";
		if (request.getSession().getAttribute("user") != null) {
			UserModel user = (UserModel) request.getSession().getAttribute("user");
			username = user.getName();
		}
	%>
	Welcome :
	<%=username%>
	<button method=>logout</button>
</body>
</html>